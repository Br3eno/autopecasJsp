<div>
    <a class="hiddenanchor" id="signup"></a>
    <a class="hiddenanchor" id="signin"></a>
    <div class="login_wrapper">
        <div class="animate form login_form">
            <section class="login_content">
                <form action="" method="post">
                    <h1>Cadastrar Login</h1>
                    <div>
                        <input type="text" name="nome" id="nome" class="form-control" placeholder="Nome" required="" />
                    </div>
                    <div class="item form-group">
                        <div>
                            <input type="email" id="email" name="email" required="required" placeholder="Email" class="form-control col-md-7 col-xs-12">
                        </div>
                    </div>
                    <div>
                        <input type="text" name="user" id="user" class="form-control" placeholder="Usuário" required="" />
                    </div>
                    <div>
                        <input type="password" name="senha" id="senha" class="form-control" placeholder="Senha" required="" />
                    </div>
                    <div>
                        <input type="submit" value="Cadastrar">
                        <button onclick="window.location.href='login.jsp'">Voltar para login</button>
                    </div>
                    <div class="clearfix"></div>
                    <div class="separator"></div>
                </form>
            </section>
        </div>
    </div>
</div>
